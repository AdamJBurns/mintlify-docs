{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "quill",
  "name": "SEO Crawler",
  "colors": {
    "primary": "#000000",
    "light": "#666666",
    "dark": "#ffffff",
    "background": {
      "light": "#fafafa",
      "dark": "#000000"
    }
  },
  "font": {
    "headings": {
      "family": "Geist",
      "weight": 600
    },
    "body": {
      "family": "Geist",
      "weight": 400
    },
    "code": {
      "family": "Geist Mono"
    }
  },
  "appearance": {
    "default": "system",
    "strict": false
  },
  "rounded": "default",
  "modeToggle": {
    "default": "dark"
  },
  "codeBlock": {
    "mode": "auto"
  },
  "sidebar": {
    "lines": "container"
  },
  "search": {
    "prompt": "Search documentation..."
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart",
              "concepts"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "features/broken-link-detection",
              "features/response-time-tracking",
              "features/redirect-chain-analysis",
              "features/ssl-monitoring",
              "features/seo-analysis",
              "features/anchor-text-analysis",
              "features/ai-review"
            ]
          },
          {
            "group": "Domain Management",
            "pages": [
              "domains/adding-domains",
              "domains/dns-verification",
              "domains/verification-troubleshooting"
            ]
          },
          {
            "group": "Crawling",
            "pages": [
              "crawling/starting-crawls",
              "crawling/scan-options",
              "crawling/crawler-whitelist",
              "crawling/crawl-results",
              "crawling/sharing-results"
            ]
          },
          {
            "group": "Plans & Billing",
            "pages": [
              "billing/plans",
              "billing/limits",
              "billing/upgrading"
            ]
          },
          {
            "group": "Agency Features",
            "pages": [
              "agency/white-labeling",
              "agency/custom-domains",
              "agency/client-management"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/webhooks",
              "integrations/email-notifications"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API",
            "pages": [
              "api-reference/index"
            ]
          }
        ]
      },
      {
        "tab": "Changelog",
        "groups": [
          {
            "group": "Updates",
            "pages": [
              "changelog"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.seocrawler.app",
          "icon": "gauge-high"
        },
        {
          "anchor": "Status",
          "href": "https://status.seocrawler.app",
          "icon": "signal-bars"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@seocrawler.app"
      }
    ],
    "primary": {
      "type": "ghost",
      "label": "Get Started â†’",
      "href": "https://app.seocrawler.app/signup"
    }
  },
  "topbar": {
    "style": "gradient"
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor"
    ]
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "footer": {
    "socials": {
      "x": "https://x.com/seocrawler",
      "github": "https://github.com/seocrawler",
      "linkedin": "https://linkedin.com/company/seocrawler"
    }
  },
  "seo": {
    "indexHiddenPages": false
  },
  "analytics": {
    "posthog": {
      "apiKey": ""
    }
  }
}
