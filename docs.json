{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "SEO Crawler",
  "colors": {
    "primary": "#000000",
    "light": "#666666",
    "dark": "#ffffff"
  },
  "font": {
    "headings": {
      "family": "Geist",
      "weight": 600
    },
    "body": {
      "family": "Geist",
      "weight": 400
    },
    "code": {
      "family": "Geist Mono"
    }
  },
  "appearance": {
    "default": "system"
  },
  "rounded": "default",
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "icon": "book-text",
        "groups": [
          {
            "group": "Getting Started",
            "icon": "rocket",
            "pages": [
              "introduction",
              "quickstart",
              "concepts"
            ]
          },
          {
            "group": "Features",
            "icon": "sparkles",
            "pages": [
              "features/broken-link-detection",
              "features/orphan-pages",
              "features/missing-from-site",
              "features/response-time-tracking",
              "features/redirect-chain-analysis",
              "features/ssl-certificate-check",
              "features/seo-analysis",
              "features/anchor-text-analysis",
              "features/ai-review",
              "features/ai-internal-link-suggestions"
            ]
          },
          {
            "group": "Domain Management",
            "icon": "globe",
            "pages": [
              "domains/adding-domains",
              "domains/dns-verification",
              "domains/verification-troubleshooting"
            ]
          },
          {
            "group": "Crawling",
            "icon": "search",
            "pages": [
              "crawling/starting-crawls",
              "crawling/automated-crawls",
              "crawling/scan-options",
              "crawling/crawler-whitelist",
              "crawling/crawl-results",
              "crawling/sharing-results"
            ]
          },
          {
            "group": "Plans & Billing",
            "icon": "credit-card",
            "pages": [
              "billing/plans",
              "billing/limits",
              "billing/upgrading"
            ]
          },
          {
            "group": "Agency Features",
            "icon": "building-2",
            "pages": [
              "agency/white-labeling",
              "agency/custom-domains",
              "agency/ssl-not-ready",
              "agency/client-management"
            ]
          },
          {
            "group": "Integrations",
            "icon": "plug",
            "pages": [
              "integrations/webhooks",
              "integrations/email-notifications"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "icon": "terminal",
        "groups": [
          {
            "group": "Overview",
            "icon": "book-open",
            "pages": [
              "api-reference/index"
            ]
          },
          {
            "group": "Endpoints",
            "icon": "code",
            "pages": [
              "api-reference/crawls",
              "api-reference/webhooks"
            ]
          }
        ]
      },
      {
        "tab": "Changelog",
        "icon": "history",
        "groups": [
          {
            "group": "Updates",
            "icon": "clock",
            "pages": [
              "changelog"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.seocrawler.app",
          "icon": "gauge"
        },
        {
          "anchor": "Status",
          "href": "https://status.seocrawler.app",
          "icon": "activity"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@seocrawler.app"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://app.seocrawler.app/signup"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor"
    ]
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "footer": {
    "socials": {
      "x": "https://x.com/seocrawler",
      "github": "https://github.com/seocrawler",
      "linkedin": "https://linkedin.com/company/seocrawler"
    }
  },
  "seo": {
    "indexHiddenPages": false
  },
  "analytics": {
    "posthog": {
      "apiKey": ""
    }
  }
}



