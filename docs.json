{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "quill",
  "name": "SEO Crawler",
  "colors": {
    "primary": "#0F0F0F",
    "light": "#0F0F0F",
    "dark": "#FAFAFA",
    "background": {
      "light": "#FFFFFF",
      "dark": "#0F0F0F"
    }
  },
  "font": {
    "headings": {
      "family": "Geist",
      "weight": 600
    },
    "body": {
      "family": "Geist",
      "weight": 400
    },
    "code": {
      "family": "Geist Mono"
    }
  },
  "appearance": {
    "default": "system"
  },
  "rounded": "lg",
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Overview",
        "icon": "house",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart",
              "concepts"
            ]
          }
        ]
      },
      {
        "tab": "Features",
        "icon": "sparkles",
        "groups": [
          {
            "group": "Link Monitoring",
            "pages": [
              "features/broken-link-detection",
              "features/redirect-chain-analysis",
              "features/orphan-pages",
              "features/missing-from-site"
            ]
          },
          {
            "group": "Performance",
            "pages": [
              "features/response-time-tracking",
              "features/ssl-certificate-check"
            ]
          },
          {
            "group": "SEO Tools",
            "pages": [
              "features/seo-analysis",
              "features/anchor-text-analysis"
            ]
          },
          {
            "group": "AI-Powered",
            "pages": [
              "features/ai-review",
              "features/ai-internal-link-suggestions"
            ]
          }
        ]
      },
      {
        "tab": "Domain & Crawling",
        "icon": "globe",
        "groups": [
          {
            "group": "Domain Management",
            "pages": [
              "domains/adding-domains",
              "domains/dns-verification",
              "domains/verification-troubleshooting"
            ]
          },
          {
            "group": "Crawling Operations",
            "pages": [
              "crawling/starting-crawls",
              "crawling/automated-crawls",
              "crawling/scan-options",
              "crawling/crawler-whitelist",
              "crawling/crawl-results",
              "crawling/sharing-results"
            ]
          }
        ]
      },
      {
        "tab": "Platform",
        "icon": "building",
        "groups": [
          {
            "group": "Plans & Billing",
            "pages": [
              "billing/plans",
              "billing/limits",
              "billing/upgrading"
            ]
          },
          {
            "group": "Agency Features",
            "pages": [
              "agency/white-labeling",
              "agency/custom-domains",
              "agency/client-management",
              "agency/ssl-not-ready"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/webhooks",
              "integrations/email-notifications"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "icon": "code",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/index",
              "api-reference/domains",
              "api-reference/crawls",
              "api-reference/webhooks"
            ]
          }
        ]
      },
      {
        "tab": "Resources",
        "icon": "book-open",
        "groups": [
          {
            "group": "Resources",
            "pages": [
              "changelog"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.seocrawler.app",
          "icon": "gauge"
        },
        {
          "anchor": "Status",
          "href": "https://status.seocrawler.app",
          "icon": "activity"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@seocrawler.app"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://app.seocrawler.app/signup"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor"
    ]
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "footer": {
    "socials": {
      "x": "https://x.com/seocrawler",
      "github": "https://github.com/seocrawler",
      "linkedin": "https://linkedin.com/company/seocrawler"
    }
  },
  "seo": {
    "indexHiddenPages": false
  },
  "analytics": {
    "posthog": {
      "apiKey": ""
    }
  }
}



